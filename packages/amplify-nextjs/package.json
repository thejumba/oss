{
  "name": "@jumba/amplify-nextjs",
  "version": "0.0.7",
  "description": "Delightful hooks and utilities for working with AWS Amplify and Next.js (app directory)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth.js",
      "types": "./dist/auth.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "amplify",
    "appsync",
    "nextjs",
    "react",
    "hooks",
    "amplify-nextjs",
    "amplify-hooks"
  ],
  "author": "Jumba Developers",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.33",
    "aws-amplify": "^5.3.11",
    "next": "^14.0.1",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "aws-amplify": ">=5",
    "next": ">=13",
    "react": ">=18"
  }
}
